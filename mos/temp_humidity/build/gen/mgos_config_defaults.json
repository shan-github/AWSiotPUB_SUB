   "enable": true,
   "reconnect_interval_min": 1,
   "reconnect_interval_max": 60
  },
  "uart": {
   "baud_rate": 115200,
   "fc_type": 2
  }
 },
 "dash": {
  "server": "wss://dash.mongoose-os.com/api/v2/rpc",
