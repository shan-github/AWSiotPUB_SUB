{
 "debug": {
  "level": 2,
  "factory_reset_gpio": -1
 },
 "device": {
  "id": "esp8266_??????"
 },
 "sys": {
  "wdt_timeout": 30,
  "atca": {
   "i2c_addr": 96,
   "ecdh_slots_mask": 12
  }
 },
 "conf_acl": "*",
 "i2c": {
  "freq": 100000,
  "sda_gpio": 12,
  "scl_gpio": 14
 },
 "mqtt": {
  "server": "iot.eclipse.org:1883",
  "reconnect_timeout_min": 2.0,
  "reconnect_timeout_max": 60.0,
  "clean_session": true,
  "keep_alive": 60,
  "max_qos": 2,
  "recv_mbuf_limit": 3072
 },
 "mqtt1": {
  "server": "iot.eclipse.org:1883",
  "reconnect_timeout_min": 2.0,
  "reconnect_timeout_max": 60.0,
  "clean_session": true,
  "keep_alive": 60,
  "max_qos": 2,
  "recv_mbuf_limit": 3072
 },
 "shadow": {
  "enable": true,
  "ota_enable": true
 },
