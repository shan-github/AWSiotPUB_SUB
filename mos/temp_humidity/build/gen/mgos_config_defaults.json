   "reconnect_timeout_max": 60
  }
 },
 "update": {
  "timeout": 300,
  "ssl_ca_file": "ca.pem",
  "enable_post": true
 },
 "rpc": {
  "enable": true,
  "max_frame_size": 4096,
  "max_queue_length": 25,
  "default_out_channel_idle_close_timeout": 10,
